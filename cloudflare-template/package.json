{
  "name": "tr2b-cloudflare-template",
  "version": "1.0.0",
  "description": "Cloudflare Workers deployment template for TR2B project",
  "main": "src/index.ts",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "build": "wrangler deploy --dry-run",
    "deploy": "wrangler deploy",
    "deploy:staging": "wrangler deploy --env staging",
    "setup": "./setup.sh",
    "update": "git pull origin main && npm install",
    "tail": "wrangler tail",
    "test": "wrangler dev --test"
  },
  "dependencies": {
    "hono": "^4.5.0",
    "@hono/node-server": "^1.12.0"
  },
  "devDependencies": {
    "wrangler": "^3.60.0",
    "@types/node": "^20.14.0",
    "@cloudflare/workers-types": "^4.20240603.0",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "tr2b",
    "react",
    "express",
    "deployment",
    "template"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/YOUR_USERNAME/TR2B-cloudflare-template.git"
  },
  "bugs": {
    "url": "https://github.com/YOUR_USERNAME/TR2B-cloudflare-template/issues"
  },
  "homepage": "https://github.com/YOUR_USERNAME/TR2B-cloudflare-template#readme"
}
